.title{
    text-align: center;
    display: flex;
    flex-wrap:wrap;
    justify-content:center;
    align-items:center;
    padding: 50rpx 0;
}
.title text{
    display: block;
}
.title text:first-child {
    font-size: 36rpx;
    color: #577eff;
}
.title text:last-child{
    color: #afafaf;
    font-size: 28rpx;
}
.title text.iconfont{
  color: #577eff;
  font-size: 30rpx;
  margin-left: 8rpx;
  font-weight: bold;
}
.money-list  {
    padding: 0 42rpx;
    display: flex;
    flex-wrap:wrap;
    justify-content:space-between;
    align-items:center;
}
.money-list .block{
  display: flex;
  justify-content:center;
  align-items:center;
  height: 130rpx;
  border:4rpx solid #577eff;
  width: 200rpx; 
  margin-bottom: 30rpx;
  border-radius: 15rpx;
}
.money-list .block text{
   font-size: 70rpx;
   color: #577eff;
   font-style:italic;
}
.money-list .block text:after{
   content: "å…ƒ";
    font-size:26rpx;
    margin-top: 15rpx;
    margin-left: 5rpx;
    color: #577eff;
}
.money-list .block.set{
    border-color: #577eff;
    background: #577eff;
}
.money-list .block.set text{
    color: #fff;
}
.money-list .block.set text:after{
    color: #fff;
}
.gratis {
  display: flex;
  justify-content:center;
  align-items:center;
  margin: 40rpx 0;
  font-size: 30rpx;
  color: #222;
}
.gratis text {
    font-size: 30rpx;
    color: #333;
}
.prompt-title {
    display: flex;
    padding: 0 30rpx;
}
.prompt-title text{
    font-size: 26rpx;
    line-height: 45rpx;
}
.prompt-title text:first-child{
    color: #d92a2a;
}
.input-text {
    display: flex;
    justify-content:space-between;
    align-items:center;
    padding:0 42rpx 52rpx 42rpx;
    height: 50rpx;
}
.input-text  text {
    font-size: 30rpx;
    color: #333;
}
.input-text  .val {
  display: flex;
  align-items:center;
  justify-content:flex-end;
  flex: 1;
  height: 50rpx;
}
.input-text .val text {
    font-size: 30rpx;
    color: #a9a9a9;
}
.input-text .val .iconfont{
    font-size: 26rpx;
}
.release{
    width: 90%;
    display: flex;
    height: 100rpx;
    background:#577eff;
    align-items: center;
    font-size:36rpx;
    color: #fff;
    justify-content: center;
    border-radius: 10rpx;
    margin-top: 32rpx;
}
.mask-back {
    position: fixed;
    top: 0;
    left: 0;
    height: 100%;
    width: 100%;
    background: #000;
    opacity: 0.6;
    z-index: 1;
}

.title-top {
    padding-bottom: 30rpx;
    display: flex;
    align-items: center;
    justify-content: center;
}
.title-top image {
    height: 32rpx;
    width: 32rpx;
}
.prompt {
   margin-bottom: 50rpx;
}

.address-box {
    line-height: 65rpx;
    text-align: center;
    font-size: 44rpx;
    color: #000;
}
.address {
    position: fixed;
    bottom: 0;
    left: 0;
    width: 100%;
    z-index: 2;
    background: #fff;
    padding: 120rpx 0 60rpx 0;
}
.picker{
  width: 100%;
  height: 340rpx;
}
.complete {
  height: 86rpx;
  width: 100%;
  border-top:1px solid #a5a5a5; 
  border-bottom:1px solid #a5a5a5; 
  position: absolute;
  top: 0;
  left: 0;
  display: flex;
  align-items: center;
  justify-content: flex-end;
  background: #fff;
  z-index: 9;
}
.complete text{
  font-size: 34rpx;
  color: #000;
  padding-right: 38rpx;
  font-weight: bold;
}
