.logo {
    display: flex;
    justify-content: center;
    padding-top: 14rpx;
    
}
.logo image {
    height: 172rpx;
    width: 725rpx;
}
.img-list {
    display: flex;
    flex-wrap: wrap;
    padding-top: 50rpx;
}
.img-list .box{
    display: flex;
    justify-content: center;
    flex-wrap: wrap;
    width: 25%;
    margin-bottom: 100rpx;
}
.img-list .box image{
    height: 70rpx;
    width: 70rpx;
}
.img-list .box text {
    width: 100%;
    display: flex;
    justify-content: center;
    font-size: 28rpx;
    padding-top: 25rpx;
}
.mores {
    height: 80rpx;
    display: flex;
    margin: 0 24rpx;
    font-size: 28rpx;
    color: #577eff;
    align-items: center;
    justify-content: center;
    border:1px solid #577eff;
    border-radius: 10rpx;
}
